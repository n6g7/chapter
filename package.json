{
  "name": "library",
  "version": "0.0.3",
  "description": "Digital library for physical books",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.prod.config.js",
    "dev": "webpack-dev-server",
    "pretest": "eslint src --ext js,jsx",
    "test": "mocha --compilers js:babel-core/register --require ./test/helper.js --recursive"
  },
  "keywords": [
    "book",
    "library"
  ],
  "author": "Nathan G <n6g7@gnab.fr>",
  "license": "MIT",
  "dependencies": {
    "immutable": "^3.8.1",
    "react": "^15.0.1",
    "react-addons-pure-render-mixin": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "redux": "^3.5.2",
    "redux-localstorage": "^0.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.5.4",
    "css-loader": "^0.23.1",
    "eslint": "^2.10.2",
    "eslint-plugin-react": "^5.1.1",
    "json-loader": "^0.5.4",
    "mocha": "^2.4.5",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.2",
    "stylus-loader": "^2.0.0",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ]
  }
}
